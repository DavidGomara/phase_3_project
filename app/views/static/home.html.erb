<html>
    <body>
        <h1>Music Review</h1>
        <h2>Find An Artist</h2>
        <h2>Add An Artist</h2>
        <h2>Leave A Review</h2>
        <nav>
            <ul>
                <% if !session[:user_id]%>
                    <li><%= link_to("Signup", signup_path)%></li>
                    <li><%= link_to("Login", login_path)%></li>
                    <li><%= button_to "Log in with Google", '/auth/google_oauth2/', method: :post%></li>
                <% else %>
                    <li><%= link_to("Musicians", artists_path)%></li>
                <% end %>
            </ul>
        </nav>
    </body>
</html>
